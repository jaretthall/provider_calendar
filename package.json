{
  "name": "clinica-provider-schedule",
  "private": true,
  "version": "1.0.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:analyze": "vite build --mode production",
    "preview": "vite preview",
    "preview:build": "npm run build && vite preview",
    "preview:prod": "npm run build:prod && vite preview",
    "clean": "rm -rf dist",
    "clean:install": "rm -rf node_modules package-lock.json && npm install",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "serve": "vite preview --port 8080 --host",
    "deploy:preview": "npm run build:prod && npm run serve"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/supabase-js": "^2.50.0",
    "bcrypt": "^6.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/html2canvas": "^1.0.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ]
}
